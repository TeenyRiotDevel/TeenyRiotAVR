#!/bin/bash
cat << "asciiart"
    ___  ________  _            ______                                 ____  _       __
   /   |/_  __/ /_(_)___  __  _/_  __/__  ___  ____  ____ _____ ____  / __ \(_)___  / /_
  / /| | / / / __/ / __ \/ / / // / / _ \/ _ \/ __ \/ __ `/ __ `/ _ \/ /_/ / / __ \/ __/
 / ___ |/ / / /_/ / / / / /_/ // / /  __/  __/ / / / /_/ / /_/ /  __/ _, _/ / /_/ / /_
/_/  |_/_/  \__/_/_/ /_/\__, //_/  \___/\___/_/ /_/\__,_/\__, /\___/_/ |_/_/\____/\__/
                       /____/                           /____/"
asciiart
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
LD_PRELOAD="$DIR/libreadline.so.6 $DIR/libusb-1.0.so.0 $DIR/libusb-0.1.so.4 $DIR/libncurses.so.5" $DIR/avrdude_bin "$@"
